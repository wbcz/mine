        "build": "rm -rf ./dist && webpack --config config/webpack/webpack.client.conf.js && webpack --config config/webpack/webpack.server.conf.js",
        "dev": "docker-compose down --volume && docker-compose up --build",
        "start": "tsc -p tsconfig-server.json && npm run copy:posts && babel src/server -w -d build/server --no-babelrc --presets=es2017 --presets=es2015,stage-3 --plugins=transform-runtime & nodemon --watch build --delay 2 build/server/index.js",
        "prod": "pm2 start src/server/index.js",
        "deploy": "scp -r ../mine/.  root@45.76.7.65:/mine/"
